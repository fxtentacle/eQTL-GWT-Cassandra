<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DockLayoutPanel styleName="layerTableView">
		<g:north size="35">
			<g:FlowPanel styleName="pagination">
				<g:Button ui:field="prevA" enabled="false">previous Page</g:Button>
				<g:TextBox ui:field="pageA" visibleLength="5">1</g:TextBox>
				<g:Button ui:field="nextA" enabled="false">next Page</g:Button>
			</g:FlowPanel>
		</g:north>
		<g:center>
			<g:ScrollPanel>
				<g:Grid styleName="dataTable" ui:field="table" height="550px" />
			</g:ScrollPanel>
		</g:center>
		<g:south size="35">
			<g:FlowPanel styleName="pagination">
				<g:Button ui:field="prevB" enabled="false">previous Page</g:Button>
				<g:TextBox ui:field="pageB" visibleLength="5">1</g:TextBox>
				<g:Button ui:field="nextB" enabled="false">next Page</g:Button>
			</g:FlowPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>   