<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:h="urn:import:de.uni_luebeck.inb.krabbenhoeft.eQTL.client">
	<g:DockLayoutPanel styleName="layerRowsContainer" ui:field="layout">
		<g:north size="35">
			<h:TabButtonRow styleName="tabButtonRow" ui:field="tabRow">
				<h:TabButton ui:field="tableTab">
					<g:Label>Table</g:Label>
				</h:TabButton>
				<h:TabButton ui:field="gaugeTab">
					<g:Label>Chromosomal Gauge</g:Label>
				</h:TabButton>
				<h:TabButton ui:field="mapTab">
					<g:Label>Chromosomal Map</g:Label>
				</h:TabButton>
				<h:TabButton ui:field="filteringTab">
					<g:Label>Processing</g:Label>
				</h:TabButton>
				<h:TabButton ui:field="rTab">
					<g:Label>R</g:Label>
				</h:TabButton>
			</h:TabButtonRow>
		</g:north>
		<g:center>
			<g:LayoutPanel ui:field="tabContents" width="100%" height="100%" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 