<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DockLayoutPanel styleName="dataSetLayerView">
		<g:west size="200">
			<g:HTMLPanel>
				<b>Key:</b>
				<g:Label ui:field="key" />
				<br />
				<b>Name:</b>
				<g:Label ui:field="name" />
				<br />
				<b>Date created:</b>
				<g:Label ui:field="dateCreated" />
				<br />
			</g:HTMLPanel>
		</g:west>
		<g:center>
			<g:ScrollPanel>
				<g:Grid ui:field="layerTable"></g:Grid>
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 